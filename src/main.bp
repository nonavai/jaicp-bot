state: view_bookings
  q: * (–º–æ–∏ –∑–∞—è–≤–∫–∏|–ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å –∑–∞—è–≤–∫–∏|—Å—Ç–∞—Ç—É—Å –∑–∞—è–≤–∫–∏|–º–æ–∏ –∑–∞–ø–∏—Å–∏) *
  script:
    var userBookings = getUserBookings($session.userId || 'anonymous');
    if (userBookings.length === 0) {
      $reactions.answer("–£ –≤–∞—Å –ø–æ–∫–∞ –Ω–µ—Ç –∞–∫—Ç–∏–≤–Ω—ã—Ö –∑–∞—è–≤–æ–∫. –•–æ—Ç–∏—Ç–µ –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ —Ç–µ—Ö–æ–±—Å–ª—É–∂–∏–≤–∞–Ω–∏–µ?");
    } else {
      var message = "üìã –í–∞—à–∏ –∑–∞—è–≤–∫–∏:\n\n";
      for (var i = 0; i < userBookings.length; i++) {
        var booking = userBookings[i];
        var statusText = "";
        switch(booking.status) {
          case 'new': statusText = "üÜï –ù–æ–≤–∞—è"; break;
          case 'confirmed': statusText = "‚úÖ –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∞"; break;
          case 'in_progress': statusText = "üîß –í —Ä–∞–±–æ—Ç–µ"; break;
          case 'completed': statusText = "‚úÖ –ó–∞–≤–µ—Ä—à–µ–Ω–∞"; break;
          default: statusText = booking.status;
        }
        message += "üÜî " + booking.id + "\n";
        message += "üöó " + booking.brand + "\n";
        message += "üìÖ " + new Date(booking.createdAt).toLocaleDateString('ru-RU') + "\n";
        message += "üìä " + statusText + "\n\n";
      }
      $reactions.answer(message + "–ï—Å—Ç—å –ª–∏ –µ—â–µ –≤–æ–ø—Ä–æ—Å—ã?");
    }

